{
  "name": "clip-man",
  "version": "1.0.0",
  "description": "A dead simple clipboard manager",
  "main": "main.js",
  "productName": "Clip Man",
  "scripts": {
    "start": "electron .",
    "dev": "concurrently \"webpack --watch\" \"gulp serve\"",
    "build": "webpack --progress && electron-packager . ClipMan --platform=darwin --arch=x64 --version=0.36.0 --icon=Icon.icns",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "clipboard",
    "buffer",
    "desktop",
    "manager"
  ],
  "author": "Kevin Kennis",
  "license": "MIT",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.24.1",
    "concurrently": "^3.5.0",
    "electron-connect": "^0.4.2",
    "electron-packager": "^7.1.0",
    "electron-prebuilt": "^1.4.13",
    "gulp": "^3.9.1",
    "webpack": "^3.3.0"
  },
  "dependencies": {
    "copy-to-clipboard": "^3.0.2",
    "menubar": "^4.1.2",
    "pouchdb": "^5.4.4",
    "pouchdb-find": "^0.10.2",
    "pouchdb-legacy-utils": "^1.0.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  }
}
